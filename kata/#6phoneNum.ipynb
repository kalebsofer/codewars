    "def create_phone_number(n):\n",
    "    fst = n[0:3]\n",
    "    snd = n[3:6]\n",
    "    trd = n[6:10]\n",
    "    pn = \"(\" + \"\".join(map(str, fst)) + \") \" + \"\".join(map(str, snd)) + \"-\" + \"\".join(map(str, trd))\n",
    "    return pn"
